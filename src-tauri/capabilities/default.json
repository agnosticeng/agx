{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "dialog:default",
    {
      "identifier": "fs:allow-write-file",
      "allow": [
        {
          "path": "*"
        }
      ]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": [
            "--path",
            "-C",
            "-q",
            "--output-format"
          ],
          "name": "binaries/clickhouse",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "shell:default"
  ]
}
